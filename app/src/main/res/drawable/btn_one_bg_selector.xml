<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/width2"/>
            <solid android:color="@color/color_11"/>
            <padding android:bottom="@dimen/width5" android:left="@dimen/width5" android:right="@dimen/width5" android:top="@dimen/width5"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/width2"/>
            <solid android:color="@color/color_11"/>
            <padding android:bottom="@dimen/width5" android:left="@dimen/width5" android:right="@dimen/width5" android:top="@dimen/width5"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/width2"/>
            <solid android:color="@color/color_18"/>
            <padding android:bottom="@dimen/width5" android:left="@dimen/width5" android:right="@dimen/width5" android:top="@dimen/width5"/>
        </shape>
    </item>
</selector>